{
    "extends": "tslint:recommended",
    "rules": {
        "interface-name": [true, "never-prefix"],
        "quotemark": [
            true, "single", "jsx-double", "avoid-template", "avoid-escape"
        ],
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "prefer-const": true,
        "no-consecutive-blank-lines": [true, 2]
    }
}